<div class="container">
  <div class="row">
    <div class="col">
    <h1 class="list-group-item list-group-item-action list-group-item-primary text-center">Completed</h1>
    <% @completed.each do |c|%>
        <li class= "list-group-item-primary">
            <%= c.description %>      
        </li>
    <% end %>      
    </div>   
    <div class="col">
    <h1 class="list-group-item list-group-item-danger text-center">Not completed</h1>
    <% @not_completed.each do |c| %>
      <li class= "list-group-item-danger">
         <%= c.description %>
      </li>      
    <% end %>      
    </div>
  </div>
</div>





